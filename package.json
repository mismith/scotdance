{
  "name": "scotdance",
  "version": "1.8.3",
  "description": "Highland dancing event tracker",
  "author": "Murray Smith <murray@mismith.info>",
  "scripts": {
    "init": "npm run build",
    "serve": "vue-cli-service serve",
    "set-version": "node ./set-version.js",
    "postset-version": "npx cap copy",
    "sync-version": "pilfer-package version > public/version.json && npm run set-version -- $npm_package_version",
    "prebuild": "npm run sync-version",
    "build": "vue-cli-service build && npx cap sync",
    "report": "yarn run build --report && open www/report.html",
    "lint": "vue-cli-service lint",
    "dev": "npm run serve -- --https",
    "release": "git commit -a -m $npm_package_version && git tag \"v${npm_package_version}\"",
    "deploy": "firebase deploy && git push origin HEAD:master --tags"
  },
  "dependencies": {
    "@capacitor/android": "^2.4.1",
    "@capacitor/cli": "^2.4.1",
    "@capacitor/core": "^2.4.1",
    "@capacitor/ios": "^2.4.1",
    "@handsontable/vue": "^4.1.1",
    "@mdi/font": "^4.9.95",
    "@mismith/firebase-tools": "^1.4.0",
    "@outluch/v-file": "^1.0.0",
    "capacitor-plugin-ios-swipe-back": "^1.0.3",
    "compare-versions": "^3.4.0",
    "core-js": "^3.4.4",
    "deep-property": "^1.1.0",
    "fast-html-parser": "^1.0.1",
    "firebase": "^7.20.0",
    "firebase-tools": "^7.12.0",
    "fuse.js": "^3.4.4",
    "handsontable": "^7.3.0",
    "lodash.find": "^4.6.0",
    "lodash.groupby": "^4.6.0",
    "lodash.orderby": "^4.6.0",
    "md5": "^2.2.1",
    "moment-mini": "^2.22.1",
    "obj-flatten": "^2.0.4",
    "roboto-fontface": "*",
    "save-csv": "^4.1.0",
    "simple-line-icons": "^2.4.1",
    "vue": "^2.6.10",
    "vue-async-computed": "^3.6.1",
    "vue-body-class": "^2.0.0-beta.2",
    "vue-localstorage": "^0.6.2",
    "vue-observe-visibility": "^0.4.6",
    "vue-router": "^3.0.2",
    "vue-scrollto": "^2.17.1",
    "vuedraggable": "^2.20.0",
    "vuefire": "^1.4.4",
    "vuetify": "^2.1.10",
    "vuex": "^3.1.0",
    "vuex-map-fields": "^1.3.2",
    "vuexfire": "^2.3.0",
    "xlsx": "^0.13.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.2.3",
    "@vue/cli-plugin-eslint": "^4.2.3",
    "@vue/cli-service": "^4.2.3",
    "@vue/eslint-config-airbnb": "^3.0.5",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.8.0",
    "eslint-plugin-vue": "^6.2.2",
    "lint-staged": "^7.2.2",
    "pilfer-package": "^1.0.0",
    "sass": "^1.26.3",
    "sass-loader": "^8.0.0",
    "vue-cli-plugin-vuetify": "^2.0.5",
    "vue-template-compiler": "^2.6.10",
    "vuetify-loader": "^1.4.3"
  },
  "vue": {
    "outputDir": "www",
    "publicPath": "",
    "transpileDependencies": [
      "@firebase",
      "vue-scrollto",
      "vuetify"
    ]
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "@vue/airbnb"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {
      "arrow-body-style": 0,
      "max-len": 0,
      "no-unused-vars": 1
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie < 11"
  ],
  "engines": {
    "node": ">= 8.11.3",
    "npm": ">= 6.4.1"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/mismith/scotdance.git"
  }
}
