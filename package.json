{
  "name": "scotdance",
  "version": "1.4.4",
  "description": "Highland dancing event tracker",
  "author": "Murray Smith <murray@mismith.info>",
  "scripts": {
    "postinstall": "npm run build && cordova platform add android && cordova platform add ios",
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "dev": "npm run serve",
    "dev:android": "npm run build && cordova emulate android",
    "dev:ios": "npm run build && cordova emulate ios",
    "build:android": "npm run build && cordova build android --release",
    "build:ios": "npm run build && cordova build ios --release",
    "build:app": "npm run build && cordova build --release",
    "devices:android": "cordova emulate android --list",
    "devices:ios": "cordova emulate ios --list",
    "icon": "cordova-icon --icon=res/icon.png",
    "splash": "cordova-splash --splash=res/splash.png"
  },
  "dependencies": {
    "@handsontable/vue": "^2.0.0",
    "cordova": "^8.1.2",
    "cordova-android": "^7.1.1",
    "cordova-ios": "^4.5.5",
    "cordova-plugin-device": "^2.0.2",
    "cordova-plugin-network-information": "^2.0.1",
    "cordova-plugin-splashscreen": "^5.0.2",
    "cordova-plugin-statusbar": "^2.4.2",
    "cordova-plugin-whitelist": "^1.3.3",
    "deep-property": "^1.1.0",
    "fast-html-parser": "^1.0.1",
    "firebase": "^4.6.0",
    "fuse.js": "^3.2.1",
    "hammerjs": "^2.0.8",
    "handsontable": "^6.1.1",
    "lodash.groupby": "^4.6.0",
    "lodash.sortby": "^4.7.0",
    "md5": "^2.2.1",
    "moment-mini": "^2.22.1",
    "obj-flatten": "^2.0.3",
    "simple-line-icons": "^2.4.1",
    "vue": "^2.5.17",
    "vue-async-computed": "^3.3.1",
    "vue-body-class": "^2.0.0-beta.2",
    "vue-localstorage": "^0.6.2",
    "vue-material": "1.0.0-beta-10.2",
    "vue-router": "^3.0.1",
    "vue-scrollto": "^2.11.0",
    "vuedraggable": "^2.16.0",
    "vuefire": "^1.4.4",
    "vuex": "^3.0.1",
    "vuex-map-fields": "^1.3.0",
    "vuexfire": "^2.3.0",
    "xlsx": "^0.13.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.0.5",
    "@vue/cli-plugin-eslint": "^3.0.5",
    "@vue/cli-service": "^3.0.5",
    "@vue/eslint-config-airbnb": "^3.0.5",
    "cordova-icon": "^1.0.0",
    "cordova-splash": "^1.0.0",
    "lint-staged": "^7.2.2",
    "node-sass": "^4.9.0",
    "sass-loader": "^7.0.1",
    "vue-template-compiler": "^2.5.17"
  },
  "vue": {
    "outputDir": "www",
    "baseUrl": ""
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "@vue/airbnb"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {
      "arrow-body-style": 0,
      "max-len": 0,
      "no-unused-vars": 1
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "cordova": {
    "plugins": {
      "cordova-plugin-whitelist": {},
      "cordova-plugin-statusbar": {},
      "cordova-plugin-splashscreen": {},
      "cordova-plugin-device": {},
      "cordova-plugin-network-information": {}
    },
    "platforms": [
      "ios",
      "android"
    ]
  },
  "engines": {
    "node": ">= 8.11.3",
    "npm": ">= 6.4.1",
    "imagemagick": "brew install imagemagick"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/mismith/scotdance.git"
  },
  "license": "UNLICENSED"
}
