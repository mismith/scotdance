{
  "name": "scotdance",
  "version": "1.4.4",
  "description": "Highland dancing event tracker",
  "author": "Murray Smith <murray@mismith.info>",
  "license": "UNLICENSED",
  "scripts": {
    "dev": "webpack-dev-server --inline --progress --config build/webpack.dev.conf.js",
    "lint": "eslint --ext .js,.vue src",
    "build": "node build/build.js",
    "report": "npm run build --report",
    "deploy": "npm run build && firebase deploy",
    "web": "npm run build && open ./www/index.html",
    "ios": "npm run build && cordova run ios",
    "android": "npm run build && cordova run android",
    "build:app": "npm run build && cordova build --release",
    "icon": "cordova-icon --icon=static/img/touchicon.png",
    "splash": "cordova-splash --splash=static/img/splash.png"
  },
  "dependencies": {
    "@handsontable/vue": "^2.0.0-beta1",
    "cordova-android": "^7.1.0",
    "cordova-ios": "^4.5.4",
    "cordova-plugin-device": "^2.0.2",
    "cordova-plugin-network-information": "^2.0.1",
    "cordova-plugin-splashscreen": "^5.0.2",
    "cordova-plugin-statusbar": "^2.4.2",
    "cordova-plugin-whitelist": "^1.3.3",
    "deep-property": "^1.1.0",
    "fast-html-parser": "^1.0.1",
    "firebase": "^4.6.0",
    "fuse.js": "^3.2.1",
    "hammerjs": "^2.0.8",
    "handsontable": "^6.1.1",
    "lodash.groupby": "^4.6.0",
    "lodash.sortby": "^4.7.0",
    "md5": "^2.2.1",
    "moment-mini": "^2.22.1",
    "obj-flatten": "^2.0.3",
