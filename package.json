{
  "name": "scotdance",
  "version": "1.7.1",
  "description": "Highland dancing event tracker",
  "author": "Murray Smith <murray@mismith.info>",
  "scripts": {
    "init": "npm run build && cordova prepare",
    "serve": "vue-cli-service serve",
    "prebuild": "pilfer-package version > public/version.json",
    "build": "vue-cli-service build --report",
    "lint": "vue-cli-service lint",
    "dev": "npm run serve",
    "dev:android": "NODE_ENV=development npm run build && cordova emulate android",
    "dev:ios": "NODE_ENV=development npm run build && cordova emulate ios",
    "devices": "cordova run --list",
    "build:android": "npm run build && cordova build android --release",
    "build:app": "npm run build && cordova build --release",
    "build:ios": "npm run build && cordova build ios --release",
    "version": "node ./cordova-version.js",
    "release": "git commit -a -m $npm_package_version && git tag \"v${npm_package_version}\"",
    "deploy": "firebase deploy && git push origin master --tags"
  },
  "dependencies": {
    "@handsontable/vue": "^2.0.0",
    "@mismith/firebase-tools": "^1.4.0",
    "@outluch/v-file": "^1.0.0",
    "compare-versions": "^3.4.0",
    "cordova": "^8.1.2",
    "cordova-android": "^7.1.4",
    "cordova-ios": "^5.0.0",
    "cordova-plugin-device": "^2.0.2",
    "cordova-plugin-network-information": "^2.0.1",
    "cordova-plugin-splashscreen": "^5.0.2",
    "cordova-plugin-statusbar": "^2.4.2",
    "cordova-plugin-whitelist": "^1.3.3",
    "cordova-plugin-wkwebview-engine": "^1.1.4",
    "deep-property": "^1.1.0",
    "fast-html-parser": "^1.0.1",
    "firebase": "^5.9.2",
    "fuse.js": "^3.4.4",
    "handsontable": "^6.1.1",
    "lodash.find": "^4.6.0",
    "lodash.groupby": "^4.6.0",
    "lodash.sortby": "^4.7.0",
    "material-design-icons-iconfont": "^4.0.5",
    "md5": "^2.2.1",
    "moment-mini": "^2.22.1",
    "obj-flatten": "^2.0.4",
    "save-csv": "^4.1.0",
    "simple-line-icons": "^2.4.1",
    "typeface-roboto": "0.0.54",
    "vue": "^2.6.10",
    "vue-async-computed": "^3.6.1",
    "vue-body-class": "^2.0.0-beta.2",
    "vue-localstorage": "^0.6.2",
    "vue-router": "^3.0.2",
    "vue-scrollto": "^2.15.0",
    "vuedraggable": "^2.20.0",
    "vuefire": "^1.4.4",
    "vuetify": "^1.5.7",
    "vuex": "^3.1.0",
    "vuex-map-fields": "^1.3.2",
    "vuexfire": "^2.3.0",
    "xlsx": "^0.13.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.5.3",
    "@vue/cli-plugin-eslint": "^3.5.1",
    "@vue/cli-service": "^3.5.3",
    "@vue/eslint-config-airbnb": "^3.0.5",
    "lint-staged": "^7.2.2",
    "node-sass": "^4.11.0",
    "pilfer-package": "^1.0.0",
    "sass-loader": "^7.0.1",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.1",
    "vue-cli-plugin-vuetify": "^0.4.6",
    "vue-template-compiler": "^2.6.10",
    "vuetify-loader": "^1.2.1"
  },
  "vue": {
    "outputDir": "www",
    "publicPath": ""
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "@vue/airbnb"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {
      "arrow-body-style": 0,
      "max-len": 0,
      "no-unused-vars": 1
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie < 11"
  ],
  "cordova": {
    "plugins": {
      "cordova-plugin-whitelist": {},
      "cordova-plugin-statusbar": {},
      "cordova-plugin-splashscreen": {},
      "cordova-plugin-device": {},
      "cordova-plugin-network-information": {},
      "cordova-plugin-wkwebview-engine": {}
    },
    "platforms": [
      "android",
      "ios"
    ]
  },
  "engines": {
    "node": ">= 8.11.3",
    "npm": ">= 6.4.1"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/mismith/scotdance.git"
  }
}
