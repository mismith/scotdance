  "dependencies": {
    "@mismith/firebase-tools": "^1.4.0",
    "firebase-admin": "^8.9.0",
    "firebase-functions": "^3.3.0",
    "postmark": "^2.2.0"
  },
  "devDependencies": {
    "firebase-functions-test": "^0.1.6",
    "firebase-tools": "^9.22.0",
    "tslint": "^6.1.3",
    "tslint-config-airbnb": "^5.11.2",
    "typescript": "^4.4.4"
  },
  "engines": {
    "node": "10"
  }
}
