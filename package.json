    "vue-localstorage": "^0.6.2",
    "vue-material": "1.0.0-beta-10.2",
    "vue-router": "^3.0.1",
    "vue-scrollto": "^2.11.0",
    "vuedraggable": "^2.16.0",
    "vuefire": "^1.4.4",
    "vuex": "^3.0.1",
    "vuex-map-fields": "^1.3.0",
    "vuexfire": "^2.3.0",
    "xlsx": "^0.13.0"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.2",
    "babel-core": "^6.22.1",
    "babel-eslint": "^8.2.1",
